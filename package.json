{
  "name": "ludi",
  "version": "0.0.0",
  "description": "Board game engine and server.",
  "main": "dist/index.js",
  "scripts": {
    "serve": "node dist/server.js",
    "build": "npm run build-sass && npm run build-ts && npm run tslint && npm run copy-static-assets",
    "build-sass": "node-sass src/public/css/main.scss dist/public/css/main.css",
    "build-ts": "tsc",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-sass": "node-sass -w src/public/css/main.scss dist/public/css/main.css",
    "watch-ts": "tsc -w",
    "watch-node": "nodemon dist/server.js",
    "debug": "npm run build && npm run watch-debug",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "serve-debug": "nodemon --inspect dist/server.js",
    "test": "jest --forceExit --coverage --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ludi-tech/ludi.git"
  },
  "keywords": [
    "game",
    "board"
  ],
  "author": "Evan MacGregor, Evan Scribner-Maclean, and William Gosse III",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ludi-tech/ludi/issues"
  },
  "homepage": "https://github.com/ludi-tech/ludi#readme",
  "dependencies": {
    "async": "^2.6.0",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "class-validator": "^0.8.1",
    "compression": "^1.7.2",
    "dotenv": "^5.0.1",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "express-flash": "0.0.2",
    "express-session": "^1.15.6",
    "express-validator": "^5.0.3",
    "lodash": "^4.17.5",
    "lusca": "^1.5.2",
    "morgan": "^1.9.0",
    "nodemailer": "^4.6.2",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth": "^1.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.4.1",
    "pug": "^2.0.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "request": "^2.85.0",
    "winston": "^3.0.0-rc2"
  },
  "devDependencies": {
    "@types/async": "^2.0.47",
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/bluebird": "^3.5.20",
    "@types/body-parser": "^1.16.8",
    "@types/compression": "0.0.35",
    "@types/dotenv": "^4.0.2",
    "@types/errorhandler": "0.0.32",
    "@types/express-session": "^1.15.8",
    "@types/jest": "^22.2.0",
    "@types/jquery": "^3.3.1",
    "@types/lodash": "^4.14.104",
    "@types/lusca": "^1.5.0",
    "@types/morgan": "^1.7.35",
    "@types/node": "^9.4.7",
    "@types/nodemailer": "^4.6.0",
    "@types/passport": "^0.4.3",
    "@types/passport-google-oauth": "^1.0.37",
    "@types/pg": "^7.4.5",
    "@types/pug": "^2.0.4",
    "@types/react": "^16.0.40",
    "@types/react-dom": "^16.0.4",
    "@types/react-redux": "^5.0.15",
    "@types/redux": "^3.6.31",
    "@types/request": "^2.47.0",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "jest": "^22.4.2",
    "node-sass": "^4.8.1",
    "nodemon": "^1.17.1",
    "shelljs": "^0.8.1",
    "supertest": "^3.0.0",
    "ts-jest": "^22.4.1",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  }
}
